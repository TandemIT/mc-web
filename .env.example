# Minecraft World Archiver Configuration

# Worlds directory path (will be mounted as Docker volume)
WORLDS_DIR=/app/worlds

# Application settings
NODE_ENV=development
PORT=3000

# Cache settings
CACHE_DURATION=300
CACHE_MAX_SIZE=100

# Rate limiting
RATE_LIMIT_WINDOW=3600
RATE_LIMIT_MAX_REQUESTS=100

# Security
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info